<div class="container">
<p id="notice"><%= notice %></p>

<h1>Settings</h1>

<table class="table table-nonfluid">
  <thead>
    <tr>
      <th>Average bill</th>
      <th>Yandex net fee</th>
      <th>Yandex share</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @settings.each do |setting| %>
      <tr>
        <td><%= setting.average_bill %></td>
        <td><%= setting.yandex_net_fee %></td>
        <td><%= setting.yandex_share %></td>
        <td><%= link_to 'Show', setting %></td>
        <td><%= link_to 'Edit', edit_setting_path(setting) %></td>
        <td><%= link_to 'Destroy', setting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Setting', new_setting_path %>
</div>

